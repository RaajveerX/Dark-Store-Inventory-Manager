<!-- Overview Page, contains two modules, Add Darkstore Panel and the Darkstore List View-->
<mvc:View
    controllerName="ui5.darkstores-app.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
>
    <Page title="Inventory">
        <content>
            <mvc:XMLView viewName="ui5.darkstores-app.view.AddDarkstorePanel" />
            <mvc:XMLView viewName="ui5.darkstores-app.view.DarkstoreList" />
        </content>
    </Page>
</mvc:View>
